{
  "name": "template-node",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/server.js",
    "queue": "nodemon src/queue.js",
    "build": "babel src --out-dir dist --copy-files  ",
    "heroku-postbuild": "sucrase ./src -d ./dist --transforms imports && sequelize db:migrate",
    "migrations": "sequelize db:migrate",
    "start": "NODE_ENV=production pm2-runtime start ecosystem.config.js",
    "pretest": "NODE_ENV=test sequelize db:migrate",
    "test": "NODE_ENV=test jest --detectOpenHandles --forceExit --maxWorkers=10",
    "pretest:watch": "NODE_ENV=test sequelize db:migrate",
    "test:watch": "NODE_ENV=test jest --watch --detectOpenHandles",
    "posttest:watch": "NODE_ENV=test sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "aws-sdk": "^2.619.0",
    "bcryptjs": "^2.4.3",
    "bull": "^3.13.0",
    "bull-board": "^0.6.0",
    "cors": "^2.8.5",
    "date-fns": "^2.9.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.10",
    "nodemailer": "^6.4.2",
    "nodemailer-express-handlebars": "^3.1.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "pm2": "^4.2.3",
    "sequelize": "^5.15.1",
    "youch": "^2.0.10",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@shelf/jest-mongodb": "^1.1.3",
    "@types/jest": "^24.0.18",
    "babel-jest": "^24.9.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "factory-girl": "^5.0.4",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "sequelize-cli": "^5.5.0",
    "sqlite3": "^4.1.0",
    "sucrase": "^3.10.1",
    "supertest": "^4.0.2"
  }
}
